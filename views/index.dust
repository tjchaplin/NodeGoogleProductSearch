                    <li><a href="#">Link</a></li>
                    <li><a href="#">Link</a></li>
                    <li><a href="#">Link</a></li>
                </ul>
            </div><!--/.well -->
        </div><!--/span-->
        <div class="span9" >
            <div class="well">
                <form class="form-search" action="/productSearch" style="margin-top: 5px;margin-bottom: 5px;">
                    <input type="text" style="width:80%; height:10%;">
                    <button id="productSearch" type="submit" class="btn btn-primary btn-large" style="margin-left: 5%; width:15%; height: 10%;">Search</button>
                </form>
            </div>
        </div><!--/span-->
    </div><!--/row-->

    <hr>

    <footer>
        <p>&copy; Company 2013</p>
    </footer>

</div><!--/.fluid-container-->

